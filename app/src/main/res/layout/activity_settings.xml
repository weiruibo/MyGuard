<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_settings"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="cn.edu.gdmec.w07150837.myguard.m10settings.SettingsActivity">

    <include layout="@layout/titlebar" />

    <cn.edu.gdmec.w07150837.myguard.m10settings.widget.SettingView
        android:id="@+id/sv_blacknumber_set"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        app:settitle="黑名单拦截设置"
        app:status_off="黑名单拦截已关闭"
        app:status_on="黑名单拦截已开启"></cn.edu.gdmec.w07150837.myguard.m10settings.widget.SettingView>

    <cn.edu.gdmec.w07150837.myguard.m10settings.widget.SettingView
        android:id="@+id/sv_applock_set"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        app:settitle="程序锁设置"
        app:status_off="程序锁已关闭"
        app:status_on="程序锁已开启"

        ></cn.edu.gdmec.w07150837.myguard.m10settings.widget.SettingView>
</LinearLayout>
